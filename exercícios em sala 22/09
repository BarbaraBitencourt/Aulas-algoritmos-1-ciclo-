"""

identificação = input("Escreve seu nome: ")
endereço = input("Escreve seu endereço: ")
cpf = input("cpf:")
renda_anual = float(input("Renda anual: "))
imposto_pago = float(input("Imposto pago: "))
imposto = (renda_anual*0.15)- imposto_pago
print("Identificação: ", identificação)
print("Endereço: ", endereço)
print("cpf: ", cpf)
print("Renda: ", str(renda_anual))
print("Imposto pago: ", str(imposto_pago))
if renda_anual <= 44.000:
    print("Isento")
else:
    print("Imposto devido: ", str (imposto))
