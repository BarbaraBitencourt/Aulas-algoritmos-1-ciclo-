"""
import math
a = input("Escreva o valor de a: ")
b = input("Escreva valor de b: ")
c = input("Escreva o valor de c: ")
a = float(a)
b = float(b)
c = float (c)
delta = (b*b-4*a*c)
if delta < 0:
    print("erro")
else:
    x1 = (-b + math.sqrt(delta)) / (2*a)
    print (x1)
    x2 = (-b - math.sqrt(delta)) / (2*a)
    print (x2)
    
